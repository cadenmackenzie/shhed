# Shhed SDK Configuration
# Copy this file to .env and fill in your actual values

# Your Shhed access key
# Get this from your Shhed web interface
KV_ACCESS_KEY=ak_your_access_key_id

# Your Shhed secret key (optional - omit for public keys only)
# Get this from your Shhed web interface
KV_SECRET_KEY=your_secret_key

# Your project ID (can be numeric ID or project name)
# Examples: "123", "proj_123", "my-project-name"
KV_PROJECT_ID=your_project_id

# Shhed API endpoint (optional - defaults to production)
# Only uncomment for development/testing
# KV_ENDPOINT=http://localhost:8000

# Express example configuration
PORT=3000 